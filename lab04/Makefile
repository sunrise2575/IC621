TARGET=main

all:
	g++ $(TARGET).cpp -o main -lpthread -Wall -O3 -lrt --std=c++11

test:
	./$(TARGET) 1073741824 1
	./$(TARGET) 2147483648 1
	./$(TARGET) 4294967296 1
	./$(TARGET) 8589934592 1
	./$(TARGET) 1073741824 2
	./$(TARGET) 2147483648 2
	./$(TARGET) 4294967296 2
	./$(TARGET) 8589934592 2
	./$(TARGET) 1073741824 4
	./$(TARGET) 2147483648 4
	./$(TARGET) 4294967296 4
	./$(TARGET) 8589934592 4
	./$(TARGET) 1073741824 8
	./$(TARGET) 2147483648 8
	./$(TARGET) 4294967296 8
	./$(TARGET) 8589934592 8

clean:
	rm -rf *.o
	rm -rf $(TARGET)
